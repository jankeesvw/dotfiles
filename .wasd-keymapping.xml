<?xml version="1.0"?>
<root>
  <deviceproductdef>
    <productname>WASD_KEYBOARD</productname>
    <productid>0x04d9</productid>
  </deviceproductdef>
  <vkopenurldef>
    <name>KeyCode::VK_OPEN_URL_APP_SCREENSAVER</name>
    <url>file:///System/Library/Frameworks/ScreenSaver.framework/Versions/A/Resources/ScreenSaverEngine.app</url>
  </vkopenurldef>
  <item>
    <name>Jankees' settings</name>
    <appendix>Custom key mapping for a WASD Mechanical Keyboard.</appendix>
    <item>
       <name>Play/Pause track with command F13</name>
       <identifier>private.jankees_music_pause_track</identifier>
       <device_only>DeviceProduct::WASD_KEYBOARD</device_only>
       <autogen>__KeyToConsumer__ KeyCode::F13, ModifierFlag::COMMAND_L, ConsumerKeyCode::MUSIC_PLAY</autogen>
     </item>
    <item>
       <name>Music Next track to F13</name>
       <identifier>private.jankees_music_next_tracl</identifier>
       <device_only>DeviceProduct::WASD_KEYBOARD</device_only>
       <autogen>__KeyToConsumer__ KeyCode::F13, ConsumerKeyCode::MUSIC_NEXT</autogen>
     </item>
    <item>
      <name>Volume Down to F14</name>
      <identifier>private.jankees_volume_down</identifier>
      <device_only>DeviceProduct::WASD_KEYBOARD</device_only>
      <autogen>__KeyToConsumer__ KeyCode::F14, ConsumerKeyCode::VOLUME_DOWN</autogen>
    </item>
    <item>
      <name>Volume Up to F15</name>
      <identifier>private.jankees_volume_up</identifier>
      <device_only>DeviceProduct::WASD_KEYBOARD</device_only>
      <autogen>__KeyToConsumer__ KeyCode::F15, ConsumerKeyCode::VOLUME_UP</autogen>
    </item>
  </item>
</root>